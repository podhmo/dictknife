default: 00 01 02 03 04 05 06

00:
	dictknife shape 00*.json > 00.output
01:
	dictknife shape 01*.json > 01.output
02:
	dictknife shape 01*.json --squash > 02.output
03:
	dictknife shape 02*.json --skiplist > 03.output
04:
	dictknife shape 02*.json --skiplist > 04.output
05:
	dictknife shape 03*.json --skiplist > 05.output
06:
	dictknife shape 03*.json --skiplist > 06.output
